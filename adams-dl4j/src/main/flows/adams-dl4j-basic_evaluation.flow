adams.flow.control.Flow
	-actor
		adams.flow.standalone.CallableActors
			-actor
				adams.flow.source.SequenceSource
					-name
					test
					-actor
						adams.flow.source.FileSupplier
							-file
							${EXAMPLE_FLOWS}/data/iris.csv
					-actor
						adams.flow.transformer.SpreadSheetFileReader
							-reader
								adams.data.io.input.CsvSpreadSheetReader
									-spreadsheet-type
										adams.data.spreadsheet.SpreadSheet
					-actor
						adams.flow.transformer.SpreadSheetRemoveRow
							-position
							inv(1-10,51-60,101-110)
					-actor
						adams.flow.transformer.Convert
							-name
							convert to ML dataset
							-conversion
								adams.data.conversion.SpreadSheetToDataset
									-class-attribute
									last
					-actor
						adams.flow.transformer.Convert
							-name
							convert to DL4J dataset
							-conversion
								adams.data.conversion.DatasetToDL4JDataset
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/iris.csv
	-actor
		adams.flow.transformer.SpreadSheetFileReader
			-reader
				adams.data.io.input.CsvSpreadSheetReader
					-spreadsheet-type
						adams.data.spreadsheet.SpreadSheet
	-actor
		adams.flow.transformer.SpreadSheetRemoveRow
			-position
			1-10,51-60,101-110
	-actor
		adams.flow.transformer.Convert
			-name
			convert to ML dataset
			-conversion
				adams.data.conversion.SpreadSheetToDataset
					-class-attribute
					last
	-actor
		adams.flow.transformer.Convert
			-name
			convert to DL4J dataset
			-conversion
				adams.data.conversion.DatasetToDL4JDataset
	-actor
		adams.flow.transformer.DL4JTrainNetwork
			-builder
				adams.data.ml.DBNBuilder
					-random-number-generator
						adams.data.random.MersenneTwister
					-distribution
						adams.data.distribution.Normal
					-activation-function
						org.deeplearning4j.nn.activation.Null
					-output-activation-function
						org.deeplearning4j.nn.activation.SoftMaxRows
	-actor
		adams.flow.transformer.DL4JEvaluateNetwork
	-actor
		adams.flow.transformer.DL4JEvaluationValues
	-actor
		adams.flow.sink.SpreadSheetDisplay
			-writer
				adams.data.io.output.NullWriter
	-flow-execution-listener
		adams.flow.execution.NullListener

