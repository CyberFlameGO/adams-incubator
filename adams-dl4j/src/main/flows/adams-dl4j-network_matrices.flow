adams.flow.control.Flow
	-annotation
	Trains a Restricted Boltzman Machine on the UCI dataset iris\nand displays the matrices.
	-actor
		adams.flow.source.FileSupplier
			-file
			${EXAMPLE_FLOWS}/data/iris.csv
	-actor
		adams.flow.transformer.SpreadSheetFileReader
			-reader
				adams.data.io.input.CsvSpreadSheetReader
					-data-row-type
						adams.data.spreadsheet.DenseDataRow
					-spreadsheet-type
						adams.ml.data.Dataset
	-actor
		adams.flow.transformer.ClassSelector
			-override
			true
	-actor
		adams.flow.transformer.Convert
			-name
			convert to DL4J dataset
			-conversion
				adams.data.conversion.DatasetToDL4JDataset
	-actor
		adams.flow.transformer.DL4JTrainNetwork
			-builder
				adams.data.ml.RBMBuilder
					-random-number-generator
						adams.data.random.MersenneTwister
					-distribution
						adams.data.distribution.Normal
					-num-hidden
					3
	-actor
		adams.flow.transformer.DL4JNetworkMatrices
	-actor
		adams.flow.control.Tee
			-name
			get name
			-tee
				adams.flow.transformer.SpreadSheetInfo
					-type
					NAME
			-tee
				adams.flow.transformer.SetVariable
					-var-name
					name
	-actor
		adams.flow.sink.DisplayPanelManager
			-provider
				adams.flow.sink.SpreadSheetDisplay
					-writer
						adams.data.io.output.NullWriter
			-entry-name-variable
			name
	-flow-execution-listener
		adams.flow.execution.NullListener

