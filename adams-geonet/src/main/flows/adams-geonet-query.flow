adams.flow.control.Flow
	-actor
		adams.flow.source.GeoNetQuery
			-query-option
			-s
			-query-option
			NZCRLZ LH.10
			-query-option
			-d
			-query-option
			300
			-query-option
			-b
			-query-option
			2012/02/07 00:20:00
	-actor
		adams.flow.control.Branch
			-branch
				adams.flow.control.Sequence
					-name
					raw display
					-actor
						adams.flow.transformer.CollectionToSequence
					-actor
						adams.flow.transformer.Convert
							-conversion
								adams.data.conversion.AnyToString
					-actor
						adams.flow.sink.Display
			-branch
				adams.flow.control.Sequence
					-name
					save to disk
					-actor
						adams.flow.sink.MiniSeedFileWriter
							-query-option
							-t
							-query-option
							text
							-query-option
							-s
							-query-option
							NZCRLZ LH.10
							-query-option
							-d
							-query-option
							300
							-query-option
							-b
							-query-option
							2012/02/07 00:20:00
							-output-dir
							${EXAMPLE_FLOWS}/output
			-branch
				adams.flow.control.Sequence
					-name
					timeseries display
					-actor
						adams.flow.transformer.Convert
							-conversion
								adams.data.conversion.MiniSeedListToTimeseries
									-query-option
									-s
									-query-option
									NZCRLZ LH.10
									-query-option
									-d
									-query-option
									300
									-query-option
									-b
									-query-option
									2012/02/07 00:20:00
					-actor
						adams.flow.sink.TimeseriesDisplay
							-x
							-3
							-writer
								adams.gui.print.NullWriter
							-axis-x
								adams.gui.visualization.core.AxisPanelOptions
									-label
									time
									-tick-generator
										adams.gui.visualization.core.axis.PeriodicityTickGenerator
									-width
									40
							-axis-y
								adams.gui.visualization.core.AxisPanelOptions
									-label
									value
									-tick-generator
										adams.gui.visualization.core.axis.SimpleTickGenerator
									-width
									60
							-color-provider
								adams.gui.visualization.core.DefaultColorProvider
	-flow-execution-listener
		adams.flow.execution.NullListener

