# Cloud provider
Provider=openstack-nova

# Cloud identity (tenantName:userName)
Identity=tenantName:userName

# Cloud credential (= password)
Credential=secret

# Cloud endpoint (host with API)
Endpoint=https://somehost:5000/v2.0
